#include <iostream>
using namespace std;
int main() {
    int grade;
    cin >> grade;
    if (grade < 38) {
        cout << grade;
        return 0;
    }
    int nextMultiple = ((grade / 5) + 1) * 5;
    if (nextMultiple - grade < 3) {
        cout << nextMultiple;
    } else {
        cout << grade;
    }
    return 0;
}